{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "_id": {
      "type": "string"
    },
    "_rev": {
      "type": "string"
    },
    "author": {
      "properties": {
        "name": {
          "type": "string"
        }
      },
      "type": ["object", "null"]
    },
    "bugs": {
      "properties": {
        "url": {
          "type": "string"
        }
      },
      "required": [
        "url"
      ],
      "type": "object"
    },
    "description": {
      "type": "string"
    },
    "latest": {
      "type": "string"
    },
    "dist_tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "homepage": {
      "type": "string"
    },
    "license": {
      "type": ["object", "null"],
      "properties": {
        "type": {
          "type": "string"
        },
        "url": {
          "type": ["string", "null"]
        }
      }
    },
    "maintainers": {
      "items": {
        "properties": {
          "email": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "name"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "name": {
      "type": "string"
    },
    "readme": {
      "type": "string"
    },
    "readmeFilename": {
      "type": "string"
    },
    "repository": {
      "properties": {
        "directory": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      },
      "required": [
        "directory",
        "type",
        "url"
      ],
      "type": "object"
    },
    "modified": {
      "format": "date-time",
      "type": "string"
    },
    "created": {
      "format": "date-time",
      "type": "string"
    },
    "users": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "_id"
  ],
  "type": "object"
}
